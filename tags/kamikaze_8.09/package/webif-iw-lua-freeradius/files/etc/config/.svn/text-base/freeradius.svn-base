
config 'websettings' 'webadmin'
	option 'userlevel' '1'
	option 'enable' '0'

config 'freeradius' 'system'
	option 'apply' '/usr/lib/lua/lua-xwrt/applys/freeradius.lua'
	option 'Simultaneous_Use' '1'
	option 'Idle_Timeout' '390'
	option 'Acct_Interim_Interval' '600'
	option 'WISPr_Bandwidth_Max_Down' '512000'
	option 'WISPr_Bandwidth_Max_Up' '25600'
	option 'Fall_Through' 'yes'

